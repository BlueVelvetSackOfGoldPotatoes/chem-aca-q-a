            "name": "Question",
            "description": "The specific question derived from the article.",
            "dataType": "sc:Text"
        },
        {
            "@type": "cr:Field",
            "name": "Options",
            "description": "Multiple-choice options for the question.",
            "dataType": "sc:ItemList",
            "itemListElement": [
            {
                "@type": "cr:Field",
                "name": "Option A",
                "dataType": "sc:Text"
            },
